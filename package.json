{
  "name": "reactor-extension-alloy",
  "version": "2.8.0-beta.0",
  "description": "The Adobe Experience Platform Web SDK allows for streaming data into the platform, syncing identities, personalizing content, and more.",
  "author": "Adobe",
  "license": "Apache-2.0",
  "scripts": {
    "prepare": "husky install",
    "prepush-msg": "echo 'Running pre-push scripts...' && exit 0",
    "precommit-msg": "echo 'Running pre-commit scripts...' && exit 0",
    "postinstall": "patch-package",
    "sandbox": "reactor-sandbox",
    "build": "./scripts/build.js",
    "build:prod": "NODE_ENV=production npm run build",
    "package": "npm run build:prod && npx @adobe/reactor-packager",
    "dev": "./scripts/dev.js",
    "lint": "eslint \"*.{js,jsx}\" \"{src,test,scripts}/**/*.{js,jsx}\" --cache --fix",
    "format": "prettier --write \"*.{js,jsx,html}\" \"{src,test,scripts}/**/*.{js,jsx,html}\"",
    "test:unit": "karma start --single-run",
    "test:unit:coverage": "karma start --single-run --reporters spec,coverage",
    "test:unit:watch": "karma start",
    "test:unit:watch:chrome": "karma start --browsers Chrome",
    "test:functional": "./scripts/functional.js",
    "test:functional:watch": "./scripts/functional.js --watch",
    "test:functional:watch:debug": "echo \"open chrome://inspect in Chrome and click 'inspect'\" && node --inspect-brk ./scripts/functional.js --watch",
    "test": "npm-run-all --parallel test:unit test:functional",
    "token": "node scripts/token.js",
    "version": "./scripts/version.sh"
  },
  "lint-staged": {
    "./*.{js,jsx}": [
      "eslint --cache --fix"
    ],
    "./{src,test,scripts}/**/*.{js,jsx}": [
      "eslint --cache --fix"
    ],
    "./*.{js,jsx,html}": [
      "prettier --write"
    ],
    "./{src,test,scripts}/**/*.{js,jsx,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@adobe/alloy": "../alloy",
    "@adobe/react-spectrum": "^3.11.0",
    "@react-spectrum/combobox": "^3.0.0-rc.0",
    "@react-stately/data": "^3.4.0",
    "@spectrum-css/typography": "^3.0.2",
    "@spectrum-icons/illustrations": "^3.2.1",
    "@spectrum-icons/workflow": "^3.2.0",
    "antd": "^4.16.1",
    "classnames": "^2.2.6",
    "clipboard-copy": "^4.0.1",
    "escape-string-regexp": "^1.0.5",
    "formik": "^2.2.6",
    "once": "^1.4.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@adobe/jwt-auth": "^0.3.3",
    "@adobe/reactor-sandbox": "^10.9.0",
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "babel-eslint": "^10.0.1",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-version": "^0.2.3",
    "chalk": "^4.1.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-ban": "^1.5.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-unused-imports": "^1.1.2",
    "fs-extra": "^9.1.0",
    "glob": "^7.1.6",
    "husky": "^6.0.0",
    "jasmine-core": "^3.6.0",
    "json": "^11.0.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-jasmine-matchers": "^4.0.2",
    "karma-rollup-preprocessor": "^7.0.5",
    "karma-spec-reporter": "0.0.32",
    "lint-staged": "^10.5.4",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "patch-package": "^6.2.2",
    "prettier": "^1.17.0",
    "puppeteer": "^7.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-istanbul": "^3.0.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "stylus": "^0.54.8",
    "testcafe": "^1.14.2"
  }
}
