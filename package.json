{
  "name": "reactor-extension-alloy",
  "version": "1.0.0",
  "description": "The Adobe Experience Platform Web SDK allows for streaming data into the platform, syncing identities, personalizing content, and more.",
  "author": "Adobe",
  "license": "Apache-2.0",
  "scripts": {
    "prepush-msg": "echo 'Running pre-push scripts...' && exit 0",
    "precommit-msg": "echo 'Running pre-commit scripts...' && exit 0",
    "sandbox": "reactor-sandbox",
    "build": "./scripts/build.js",
    "build:prod": "NODE_ENV=production npm run build",
    "package": "npm run build:prod && npx @adobe/reactor-packager",
    "dev": "./scripts/dev.js",
    "lint": "eslint \"*.js\" \"src/**/*.{js,jsx}\" \"test/**/*.{js,jsx}\" \"scripts/**/*.js\"",
    "format": "prettier --write \"*.{js,jsx}\" \"{src,test}/**/*.{js,jsx}\" \"scripts/**/*.js\"",
    "test:unit": "karma start --single-run",
    "test:unit:coverage": "karma start --single-run --reporters spec,coverage",
    "test:unit:watch": "karma start",
    "test:unit:watch:chrome": "karma start --browsers Chrome",
    "test:functional": "./scripts/functional.js",
    "test:functional:watch": "./scripts/functional.js --watch",
    "test:functional:watch:debug": "echo \"open chrome://inspect in Chrome and click 'inspect'\" && node --inspect-brk ./scripts/functional.js --watch",
    "test": "npm-run-all --parallel test:unit test:functional",
    "token": "node scripts/token.js"
  },
  "pre-commit": [
    "precommit-msg",
    "format",
    "lint"
  ],
  "pre-push": [
    "prepush-msg",
    "test"
  ],
  "dependencies": {
    "@react/react-spectrum": "^2.28.0",
    "antd": "^4.0.3",
    "classnames": "^2.2.6",
    "formik": "^1.5.7",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.8.6",
    "regenerator-runtime": "^0.13.2",
    "uuid": "^7.0.3",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@adobe/jwt-auth": "^0.3.1",
    "@adobe/reactor-sandbox": "^10.8.1",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-plugin-jsx-remove-data-test-id": "^2.1.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "chalk": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "glob": "^7.1.4",
    "jasmine-core": "^3.4.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-jasmine-matchers": "^4.0.1",
    "karma-rollup-preprocessor": "^7.0.0",
    "karma-spec-reporter": "0.0.32",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "prettier": "^1.17.0",
    "puppeteer": "^1.15.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.11.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-resolve": "^4.2.3",
    "stylus": "^0.54.5",
    "testcafe": "^1.1.4"
  }
}
